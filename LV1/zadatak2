print("Upisi ocjenu: ")
a = float(input())

if a < 0 or a > 1:
    print("Broj je veci od 1 ili manji od 0")
    breakpoint
elif a < 0.6:
    print("F")
elif a >= 0.6 and a < 0.7:
    print("D")
elif a >= 0.7 and a < 0.8:
    print("C")
elif a >= 0.8 and a < 0.9:
    print("B")
elif a >= 0.9:
    print("A")
